<!DOCTYPE html>
<html lang="en">
<head>
    <title>Social Login Authentication</title>
</head>
<body>
<script type="text/javascript">
    if (window.opener) {
        window.opener.focus();

        var type = '<%= type %>';
        var state = '<%= state %>';
        var data = JSON.parse('<%-JSON.stringify(data)%>');
        console.log(data);
        if (window.opener.authState) {
            window.opener.authState(type, state, data); 
        }
    }
    window.close();
</script>
</body>
</html>
